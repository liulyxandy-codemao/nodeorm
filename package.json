{
  "name": "@liulyxandy/nodeorm",
  "version": "1.0.0",
  "description": "Node.JS ORM",
  "main": "lib/index.js",
  "scripts": {
    "start": "node .",
    "compile": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dtsc": "dtsc --project tsconfig.dtsc.json",
    "build": "npm run compile && npm run dtsc",
    "dev": "ts-node-dev -r tsconfig-paths/register ./src/test.ts",
    "pub": "npm publish --access public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liulyxandy-codemao/nodeorm.git"
  },
  "keywords": [
    "ts",
    "dev",
    "template"
  ],
  "author": "liulyxandy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/liulyxandy-codemao/nodeorm/issues"
  },
  "homepage": "http://liulyxandy.cn/",
  "devDependencies": {
    "@types/node": "latest",
    "dtsc": "^2.2.3",
    "ts-node-dev": "latest",
    "tsc-alias": "latest",
    "tsconfig-paths": "latest",
    "typescript": "latest"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/**/LICENSE"
  ],
  "dependencies": {
    "@types/mysql": "^2.15.25",
    "mysql": "^2.18.1"
  }
}
